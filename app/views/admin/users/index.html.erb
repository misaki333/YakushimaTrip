			<h2>会員一覧</h2>

				<table>
  					<thead>
    					<tr>
      						<th><b>ID</b></th>
      						<th><b>会員名</b></th>
      						<th><b>メールアドレス</b></th>
    					</tr>
  					</thead>
  					<% @user.each do |user| %>
  					<tbody>
    					<tr>
      						<td><%= user.id %></td>
      						<td><%= link_to(admin_user_path(user)) do %>
									<div><%= user.name %></div>
								<% end %>
							</td>
      						<td><%= user.email %></td>
    					</tr>
    					<% end %>
  					</tbody>
				</table>
   <div class="row">
	<div class="col-xs-6 col-xs-offset-3">
	 <p id="pageTop"><a href="#">ページのトップへ戻る</a></p>
	</div>
   </div>
</div>
